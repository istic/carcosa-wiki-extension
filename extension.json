{
	"name": "AlbaCustomStuff",
	"author": [
		"Your Name"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:AlbaCustomStuff",
	"descriptionmsg": "AlbaCustomStuff-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.30.0"
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\AlbaCustomStuff\\Hooks": "includes/Hooks.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\AlbaCustomStuff\\": "includes/"
	},
	"config": {
		"AlbaCustomStuffEnableFoo": {
			"description": "Enable the Foo feature.",
			"value": true
		}
	},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\AlbaCustomStuff\\Hooks::onBeforePageDisplay"
	},
	"MessagesDirs": {
		"AlbaCustomStuff": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "AlbaCustomStuff/resources"
	},
  	"ResourceModules": {
		"w.Global.styles": {
			"styles": [
				"x.globalstyles.css"
			]
		},
		"x.AlbaCustomStuff.styles": {
			"styles": [
				"x.alba.css"
			]
		},
		"x.SisyphusCustomStuff.styles": {
			"styles": [
				"x.sisyphus.css"
			]
		},
		"x.ForsakenCustomStuff.styles": {
			"styles": [
				"x.forsaken.css"
			]
		},
		"x.T67CustomStuff.styles": {
			"styles": [
				"x.t67.css"
			]
		},
		"z.AlbaInternalCustomStuff.styles": {
			"styles": [
				"x.albainternal.css"
			]
		}
	},
	"manifest_version": 2
}
